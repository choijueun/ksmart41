<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="k1.smart.team.mapper.csh.UserRegMapper">
	
<select id="getAllUserRegList" parameterType="String" resultType="UserReg">
	SELECT 
		 userRegCode
		,mainBusinessCode
		,userLevelCode
		,userId
		,userPw
		,userName
		,userGender
		,userAddr
		,userMoblie
		,userEmail
		,userPosition
		,userRegDate
		,userRegStatus
		,userRegComment
	FROM 
		k1_tb_user_register;
</select>

<select id="getUserRegDetail" parameterType="String" resultType="UserReg">
	SELECT 
		 userRegCode
		,mainBusinessCode
		,userLevelCode
		,userId
		,userPw
		,userName
		,userGender
		,userAddr
		,userMoblie
		,userEmail
		,userPosition
		,userRegDate
		,userRegStatus
		,userRegComment
	FROM 
		k1_tb_user_register
	WHERE 
		userRegCode = #{userRegCode};
</select>
	
	
	

</mapper>